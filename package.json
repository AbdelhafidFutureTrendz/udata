  "main": "index.js",
  "scripts": {
    "assets:watch": "webpack --mode development --progress --watch",
    "assets:build": "webpack --mode production --progress",
    "widgets:watch": "webpack --mode development --progress --watch --config webpack.config.widgets.js",
    "widgets:build": "webpack --mode production --progress --config webpack.config.widgets.prod.js",
    "oembed:watch": "webpack  --mode development --progress --watch --config webpack.config.oembed.js",
    "oembed:build": "webpack --mode production --progress --config webpack.config.oembed.js",
    "build": "npm run assets:build && npm run widgets:build && npm oembed:build",
    "test": "npm run test:unit",
    "test:watch": "export NODE_ENV=test && WATCH=true karma start",
    "test:unit": "export NODE_ENV=test && karma start --single-run",
    "lint": "eslint js/ --ext .vue,.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/etalab/udata.git"
